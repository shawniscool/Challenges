<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="challenge_form">
        <%= render 'shared/challenge_form' %>
      </section>
    </aside>
    <div>
      <h3>Challenge Feed</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
<% end %>	